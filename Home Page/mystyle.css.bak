}  

.footer-subscribe {
    margin-top: 20px;
	background-color: #b9037b;
	background: rgba(185, 3, 123, 0.08);
    color: #3B3B3B;
	padding: 60px 0px 70px;
	text-align: center;
	font-family:lucida sans unicode;
	font-size: 14px;
}
		
.footer-subscribe input[type="email"] {
    padding: 10px;
    width: 400px;
    border: none;
    border-radius: 5px;
}
		
.footer-subscribe input[type="submit"] {
    padding: 10px 20px;
    background-color: #b9037b;
	color:#ffffff;
	font-weight:bold;
	border: none;
	border-radius: 5px;
	cursor: pointer;
	font-size: 13px;
}

footer {
    background-color: #b9037b;
    color: white;
    text-align: center;
    position: relative;
    bottom: 0;
    width: 100%;
	font-family: sans-serif;
	font-size: 14px;
}

footer a {
    color: white;
    text-decoration: none;
    padding: 0 10px;
}
        
footer a:hover {
    color: black;
}
        
.footer-content {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
	flex-wrap: wrap;
    max-width: 1600px;
    margin: 0 auto;
	font-family:lucida sans unicode;
}
        
.footer-section {
    flex: 1;
    padding: 40px;
    text-align: left;
    line-height: 1.9;
}
        
/* Media query for responsiveness */
@media (max-width: 768px) {
.footer-section {
    flex: 100%;
    text-align: center;
    }
}
