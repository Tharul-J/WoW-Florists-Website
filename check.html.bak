            <input type="text" id="email" name="email" placeholder="kavee@gmail.com" class="txt">
            <label for="adr"><i class="fa fa-address-card-o"></i> Address</label>
            <input type="text" id="adr" name="address" placeholder="No.50,Passara road,Badulla" class="txt">
            <label for="city"><i class="fa fa-institution"></i> Town</label>
            <input type="text" id="town" name="city" placeholder="Badulla" class="txt">
			<div class="row">
              <div class="col-50">
                <label for="PC">Postal Code</label>
                <input type="text" id="PC" name="PC" placeholder="90000" class="txt">
              </div>
            </div>
          </div>
		  <div class="col-50">
            <br><h3>Payment</h3><br>
            <label for="cname">Card Type</label><br>
            <input type="radio" name="card" value="visa"> <i class="fa fa-cc-visa" style="color:navy;"></i> Visa &nbsp&nbsp&nbsp&nbsp&nbsp
			<input type="radio" name="card" value="master"> <i class="fa fa-cc-mastercard" style="color:red;"></i> Mastercard &nbsp&nbsp&nbsp&nbsp&nbsp
			<input type="radio" name="card" value="amex"> <i class="fa fa-cc-amex" style="color:blue;"></i> American Express &nbsp&nbsp&nbsp&nbsp&nbsp
			<input type="radio" name="card" value="discover"> <i class="fa fa-cc-discover" style="color:orange;"></i> Discover<br><br><br>
            <label for="ccnum">Credit card number</label>
            <input type="text" id="ccnum" name="cardnumber" placeholder="1111 2222 3333 4444" class="txt">
            <label for="expmonth">Expiration Month</label>
            <select name="month">
				<option disabled selected style="display:none;">Select the month...</option>
				<option value="1">January</option>
				<option value="2">February</option>
				<option value="3">March</option>
				<option value="4">April</option>
				<option value="5">May</option>
				<option value="6">June</option>
				<option value="7">July</option>
				<option value="8">August</option>
				<option value="9">September</option>
				<option value="10">October</option>
				<option value="11">November</option>
				<option value="12">December</option>
			</select><br><br>
            <div class="row">
              <div class="col-50">
                <label for="expyear">Expiration Year</label>
                <select name="year">
					<option disabled selected style="display:none;">Select the year...</option>
					<option value="a">2024</option>
					<option value="b">2025</option>
					<option value="c">2026</option>
					<option value="d">2027</option>
					<option value="e">2028</option>
					<option value="f">2029</option>
					<option value="g">2030</option>
				</select>
              </div>
              <div class="col-50">
                <label for="cvv">CVN</label>
                <input type="text" id="cvn" name="cvn" placeholder="352" class="txt">
              </div>
            </div>
          </div>
